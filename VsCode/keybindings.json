[
    {
        "key": "shift+alt+p",
        "command": "prettier.createConfigFile"
    },
    {
        "key": "ctrl+alt+p",
        "command": "prettier.forceFormatDocument"
    },
    {
        "key": "ctrl+n",
        "command": "explorer.newFile"
    },
    {
        "key": "ctrl+alt+n",
        "command": "explorer.newFolder"
    }

]